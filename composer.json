{
    "name": "matok/pay-by-square-svk",
    "description": "Payment QR Code generator for Slovak banks.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "paybysquare",
        "qrcode",
        "iban",
        "365bank",
        "csob",
        "fio",
        "mbank",
        "slsp",
        "primabanka",
        "tartabanka",
        "transakcnadanjepicovina"
    ],
    "authors": [
        {
            "name": "Matej Kuna",
            "email": "mat.kuna@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "endroid/qr-code": "^6.0",
        "symfony/process": "^7.2",
        "symfony/string": "^7.2"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.45",
        "phpunit/phpunit": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Matok\\PayBySquare\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
